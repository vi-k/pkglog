    "version": "0.2.0",
    "configurations": [
        {
            "name": "pkglog minimal",
            "request": "launch",
            "type": "dart",
            "program": "example/pkglog_minimal_example.dart"
        },
        {
            "name": "pkglog",
            "request": "launch",
            "type": "dart",
            "cwd": "example/pkglog_example",
            "program": "bin/pkglog_example.dart"
        },
        {
            "name": "pkglog test asserts (enabled)",
            "request": "launch",
            "type": "dart",
            "program": "test/pkglog_test_asserts.dart",
            "args": [
                "--enable-asserts"
            ]
        },
        {
            "name": "pkglog test asserts (disabled)",
            "request": "launch",
            "type": "dart",
            "program": "test/pkglog_test_asserts.dart"
        }
    ]
}
